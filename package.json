{
  "name": "sync-tool",
  "private": true,
  "version": "1.0.8",
  "type": "module",
  "author": "forget-pro <zippy.forget@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && electron-builder",
    "preview": "vite preview",
